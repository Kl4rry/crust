let failed = 0

for test in (echo tests/*.crust) {
    let output = $null;
    try {
        $output = (crust $test)
    } catch {
        print ($output)
        print "$test Failed"
        $failed += 1 
    }
}

if $failed == 0 {
    print 'Passed all tests'
} else {
    print "$failed tests failed"
}